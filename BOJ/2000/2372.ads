with Ada.Text_IO; use Ada.Text_IO;

procedure program is
begin
  Put_Line("Animal      Count");
  Put_Line("-----------------");
  Put_Line("Chickens      100");
  Put_Line("Clydesdales     5");
  Put_Line("Cows           40");
  Put_Line("Goats          22");
